

$ docker build . -t wasm-engines


# docker needs --security-opt because one of the engines (life or wasmer?) uses an uncommon syscall (userfaultfd??)

$ docker run -it --entrypoint=/bin/bash -v $(pwd)/testresults:/testresults --security-opt seccomp=./seccompprofile.json wasm-engines

$ PYTHONIOENCODING=UTF-8 python3 ewasm.py